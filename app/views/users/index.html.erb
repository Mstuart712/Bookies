<h1>Hello from all the users</h1>
<% @users.each do |u| %> 

<div id="user-div">
<span><%= image_tag avatar_url(u) %>
<span><%= u.username %></span>
<span><%= u.email %></span>
<span><%= u.fullname %></span>
<% if u.id == session[:user_id] %>
  <span><a href="/users/<%= u.id %>/edit">edit</a></span> 
<% end %>
</div>


<% end %>
