<h1><%= @bookmark.title %></h1>
<div class=bookmark-desc>
  <a href="<%= @bookmark.url %>"><%= @bookmark.title %></a>
</div>
<div class=bookmark-details>
  <div class=bookmark-desc>
    <%= @bookmark.desc %>
  </div>
  <div class=bookmark-tags>
    tags:  <%= @bookmark.tags.join(", ") %>
  </div>
</div>
